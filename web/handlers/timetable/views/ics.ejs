BEGIN:VCALENDAR
METHOD:PUBLISH
VERSION:2.0
X-WR-CALNAME:Teaching Timetable
PRODID:-//NottingTone//tomatotabling//EN
X-APPLE-CALENDAR-COLOR:#CC73E1
X-WR-TIMEZONE:Asia/Shanghai
CALSCALE:GREGORIAN
BEGIN:VTIMEZONE
TZID:Asia/Shanghai
BEGIN:STANDARD
TZOFFSETFROM:+0900
RRULE:FREQ=YEARLY;UNTIL=19910914T150000Z;BYMONTH=9;BYDAY=3SU
DTSTART:19890917T000000
TZNAME:GMT+8
TZOFFSETTO:+0800
END:STANDARD
BEGIN:DAYLIGHT
TZOFFSETFROM:+0800
DTSTART:19910414T000000
TZNAME:GMT+8
TZOFFSETTO:+0900
RDATE:19910414T000000
END:DAYLIGHT
END:VTIMEZONE<% events.forEach(function (event) { %>
BEGIN:VEVENT
CREATED:<%- toISO(now) %>
UID:<%- event.uid %>
DTEND;TZID=Asia/Shanghai:<%- toISO(event.end) %>
TRANSP:OPAQUE
X-APPLE-TRAVEL-ADVISORY-BEHAVIOR:AUTOMATIC
SUMMARY:<%- event.name %>
DTSTART;TZID=Asia/Shanghai:<%- toISO(event.start) %>
DTSTAMP:<%- toISO(now) %>
LOCATION:<%- event.location %>
LAST-MODIFIED:<%- toISO(now) %>
SEQUENCE:0
END:VEVENT<% }); %>
END:VCALENDAR
